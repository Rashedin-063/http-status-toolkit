{
  "name": "http-status-toolkit",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./messages": {
      "import": "./dist/statusMessages.js",
      "require": "./dist/statusMessages.cjs"
    },
    "./detailed-messages": {
      "import": "./dist/detailedStatusMessages.js",
      "require": "./dist/detailedStatusMessages.cjs"
    },
    "./get-message": {
      "import": "./dist/getStatusMessage.js",
      "require": "./dist/getStatusMessage.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "docs"
  ],
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "check": "npm run build && npm run test"
  },
  "keywords": [
    "http",
    "status-codes",
    "http-status",
    "node",
    "express",
    "typescript",
    "rest-api"
  ],
  "author": "Rashedin Islam <rashedinislam.06@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rashedin-063/http-status-toolkit.git"
  },
  "homepage": "https://github.com/Rashedin-063/http-status-toolkit#readme",
  "description": "A lightweight and developer-friendly toolkit for working with HTTP status codes and messages in Node.js and TypeScript.",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
